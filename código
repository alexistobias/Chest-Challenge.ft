void setup() { 
  size(500, 500);
}

void draw() {
  for (int j = 1; j<11;j= j+2) {
    for (int i=1; i<11; i++) {
      if ( i%2==0) {
        fill(255);
      } else {
        fill(0);
      }
      rect(50*(i-1), 50*(j-1), 50, 50);
    }
  }
  for(int j = 1; j<12; j= j+2) {
  for (int i=1; i<12; i++) {
    if (i%2==0) {
      fill(0);
    } else {
      fill(255);
    }
    rect(50*(i-1), 50*(j-2), 50, 50);
  }
}
}
